function(doc) {
    var arr = new Array();
    arr[0] = doc.buildinfo.version;
    arr[1] = doc.name;
    arr[2] = doc.info.test_name;
    arr[3] = doc.time;
    arr[4] = doc.info.json;
    if(doc.info && doc.info.reb_dur)
    {
        arr[5] = doc.info.reb_dur
    }
    else
    {
        arr[5] = 0
    }
    emit(arr,doc.info.test_time);
}
